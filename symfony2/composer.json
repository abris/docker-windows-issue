{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "php": "^7.4",
        "php-translation/symfony-bundle": "^0.6",
        "ext-json": "*",
        "symfony/symfony": "~3.3",
        "doctrine/orm": "^2.5",
        "doctrine/dbal": "^2.5",
        "doctrine/doctrine-bundle": "1.6.*",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "^3.6",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/rest-bundle": "~1.5",
        "jms/serializer-bundle": "^2.4.4",
        "nelmio/api-doc-bundle": "~2.8",
        "gedmo/doctrine-extensions": "~2.4",
        "thephalcons/amazon-webservices-bundle": "dev-dev-master",
        "misd/phone-number-bundle": "~1.0",
        "royopa/fpdf-symfony2": "@dev",
        "xero/xerooauth-php": "dev-master",
        "dompdf/dompdf": "0.6.*",
        "php-mime-mail-parser/php-mime-mail-parser":"2.*",
        "tecnickcom/tcpdf": "@dev",
        "friendsofsymfony/user-bundle": "~2.0",
        "guzzlehttp/guzzle": "^6.4",
        "aws/aws-sdk-php": "^3.116",
        "google/recaptcha": "~1.1",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "gocardless/gocardless-pro": "^0.9.4",
        "adoy/oauth2": "^1.3",
        "mikehaertl/phpwkhtmltopdf": "^2.2",
        "kunalvarma05/dropbox-php-sdk": "^0.2.0",
        "stripe/stripe-php": "^7.0",
        "mustache/mustache": "^2.12",
        "nelmio/cors-bundle": "^1.5",
        "paragraph1/php-fcm": "^0.7.0",
        "drewm/mailchimp-api": "^2.5",
        "kriswallsmith/buzz": "^0.16.1",
        "calcinai/xero-php": "^1.8",
        "symfony/security-acl": "^3.0",
        "happyr/google-api-bundle": "^2.3",
        "yoti/yoti-php-sdk": "^2.5",
        "xeroapi/xero-php-oauth2": "2.2",
        "mandrill/mandrill": "^1.0",
        "chillerlan/php-qrcode": "^4.3",
        "twilio/sdk": "^6.32"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1",
        "phpunit/phpunit": "4.*",
        "phpmd/phpmd" : "@stable",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-assets-install": "relative"
    }
}
